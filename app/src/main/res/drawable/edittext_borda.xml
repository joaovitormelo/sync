<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Fundo branco -->
    <solid android:color="#FFFFFF" />

    <!-- Borda verde -->
    <stroke
        android:width="2dp"
        android:color="#D4009B94" />

    <!-- Cantos arredondados  -->
    <corners android:radius="12dp" />


    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />
</shape>
